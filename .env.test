# ResourceDeleter Lambda Environment Configuration
# Copy this file to .env and update with your actual values

# Runtime Database Configuration (set by deployment pipeline)
DB_HOST=your-postgresql-host.amazonaws.com
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_secure_password

# Test Database Configuration (for local integration tests)
FHM_REPORTINGDB_HOST=localhost
FHM_REPORTINGDB_PORT=5432
FHM_REPORTINGDB_NAME=reportingdb
FHM_REPORTINGDB_USER=postgres
FHM_REPORTINGDB_PASSWORD=Test12345

# Set to 'true' to run integration tests
RUN_INTEGRATION_TESTS=true
